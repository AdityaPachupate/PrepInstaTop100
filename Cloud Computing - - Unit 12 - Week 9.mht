div.qt-warning { border: 1px solid rgb(0, 0, 0); padding: 4px; background-color: rgb(255, 255, 183); color: rgb(255, 0, 0); }

.qt-hint, .qt-check-answer { display: inline-block; margin: 10px 0px; }

.qt-points { float: right; font-weight: bold; }

.qt-choice-selection { display: table-cell; vertical-align: middle; }

.qt-single-choice { padding-top: 5px; padding-bottom: 5px; }

.qt-choice-selection-label { display: table-cell; padding-left: 10px; }

.qt-grade-scored-lesson, .qt-grade-report, .qt-grade-assessment { margin-bottom: 10px; }

.feedback-header { margin: 5px 0px 0px; font-size: 100%; font-style: normal; }

.feedback-header .correct { color: green; }

.feedback-header .partially-correct { color: rgb(157, 151, 36); }

.feedback-header .incorrect { color: rgb(230, 0, 0); }

.feedback-header .correct-answer-heading { font-weight: normal; }

ul.correct-choices { margin: 0px; list-style-type: none; }

.gcb-mcq-choice { margin-left: 25px; }

.gcb-mcq-choice label { display: inline; }

.faculty-answer { color: green; }

.total_score { color: maroon; }

.assessment-buttons { display: flex; gap: 5%; margin-top: 50px; }
